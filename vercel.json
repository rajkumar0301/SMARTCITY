
{
  "version": 2,
  "builds": [
     {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
